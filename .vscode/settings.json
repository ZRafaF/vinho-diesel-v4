{
    "cSpell.language": "en,pt-BR",
    "cSpell.words": ["Destal", "mplx", "PWMA", "Selec", "STBY"],
    "licenser.author": "Rafael Farias",
    "licenser.license": "AL2",
    "licenser.projectName": "Vinho Diesel v4",
    "licenser.customHeader": "@FILENAME@ - @PROJECT@\n\nCopyright @YEAR@ @AUTHOR@\n\nThis is my license...",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "prettier.tabWidth": 4,
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google,  ColumnLimit: 0, IndentWidth: 4, TabWidth: 4}",
    "files.associations": {
        "*.pl": "prolog",
        "array": "cpp",
        "deque": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "vector": "cpp",
        "string_view": "cpp",
        "initializer_list": "cpp",
        "random": "cpp",
        "cmath": "cpp"
    }
}
